
+UseOS = "el9"
+JobCategory = "short"


executable              = /bin/bash
arguments               = $(script)
log                     = run-$(Process).log
output                  = run-$(Process).out
error                   = run-$(Process).err

request_cpus            = 3
request_memory          = 7G
request_disk            = 1G

queue script from scripts.txt
