+UseOS = "el9"
+JobCategory = "short"


executable              = /bin/bash
arguments               = $(script)
log                     = run-$(Process).log
output                  = run-$(Process).out
error                   = run-$(Process).err

request_cpus            = 4
request_memory          = 6G
request_disk            = 1G

queue script from scripts.txt
                                       
